                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "SELECT \n    athletes.first_name AS athlete,\n    results.value AS distance,\n    results.date AS time,\n    SUM(results.value::float) OVER (\n        PARTITION BY athletes.id \n        ORDER BY results.date\n    ) AS cumulative_distance\nFROM results \nINNER JOIN athletes ON results.athlete_id = athletes.id \nINNER JOIN metrics ON results.metric_id = metrics.id \nWHERE metrics.id = 'distance_run' \nORDER BY time;",
                    "refId": "A",
                    "sql": {
